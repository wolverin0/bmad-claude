# BMAD Claude Environment Configuration
# Copy this file to .env and fill in your values

# Gemini API Key (optional - only needed for advanced MCP features)
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your-gemini-api-key-here

# Claude API Key (optional - for future features)
# CLAUDE_API_KEY=your-claude-api-key-here

# Debug Mode (optional)
# Set to true to enable debug logging
# BMAD_DEBUG=false

# Custom Personas Directory (optional)
# Path to directory containing custom persona definitions
# BMAD_PERSONAS_DIR=~/.bmad/personas